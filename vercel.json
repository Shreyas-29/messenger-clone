{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        }
    ],
    "version": 2,
    "env": {
        "NEXT_TELEMETRY_DISABLED": "1"
    },
    "build": {
        "commands": [
            {
                "command": "prisma generate"
            },
            {
                "command": "npm run build"
            }
        ],
        "use": "@vercel/node"
    }
}